<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/certificate/element/certificatenumber/db" VERSION="20250311" COMMENT="XMLDB file for Moodle admin/tool/certificate/element/certificatenumber"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="tool_certificate_number" COMMENT="Stores copied certificate issues for certificate numbering.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="issueid" TYPE="int" LENGTH="10" NOTNULL="true"/>
        <FIELD NAME="data" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="certificatenumber" TYPE="char" LENGTH="255" NOTNULL="true"/>
        <FIELD NAME="code" TYPE="char" LENGTH="255" NOTNULL="true"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true"/>
        <FIELD NAME="expires" TYPE="int" LENGTH="10" NOTNULL="false"/>
    </FIELDS>

    <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="issueid_fk" TYPE="foreign" FIELDS="issueid" REFTABLE="tool_certificate_issues" REFFIELDS="id" ONDELETE="cascade"/>
    </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
